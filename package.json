{
  "name": "mern-stack",
  "version": "1.0.0",
  "description": "MERN Full Stack Web Application",
  "main": "server.js",
  "scripts": {
    "install-back": "npm install",
    "install-front": "npm install --prefix client",
    "intall-lib": "npm run install-back && npm run install-front",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mostafamahmoudabdelaleem/mern-stack.git"
  },
  "author": "Mostafa Mahmoud",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mostafamahmoudabdelaleem/mern-stack/issues"
  },
  "homepage": "https://github.com/mostafamahmoudabdelaleem/mern-stack#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "express": "^4.17.3",
    "mongoose": "^5.9.5",
    "reactstrap": "^8.4.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
}
